{% if catInfo.catThumbnail %}
	<style>
	#banner{ background-image: url('{{ base_url() }}/frontEnd/uploads/images/category/{{ catInfo.catThumbnail }}'); }
	</style>
{% endif  %}
<!--
	To use a video as your background, set data-video to the name of your video without
		its extension (eg. images/banner). Your video must be available in both .mp4 and .webm
		formats to work correctly.
-->
	<section id="banner" class="qna-banner">
		<div id="qna-banner">
			<div class="inner">
				<header>
					<h1>{{ catInfo.title }}</h1>
					<form action="{{ path_for('search') }}" method="post" enctype="multipart/form-data">
						<input type="search" name="searchString" class="search-box" placeholder="Enter your question ..">
						<input type="submit" value="Search" class="btn btn-large search-botton" />
						{{ csrf.field | raw }}
					</form>
					<p><strong>Find the Q/A below or do the search again.</strong></p>
	
				</header>
				<a href="#main" class="more">Learn More</a>
			</div>
		</div>
	</section>
	<div id="main">
		<div class="inner">